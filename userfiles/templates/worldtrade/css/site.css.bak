	line-height:16px;
}
.font_size_10, .font_size_10 i, .font_size_10 span {
	font-size:10px;
	line-height:14px;
}
.text_align_right {
	text-align:right;
	display:block;
}

.right_col.right {
	position:relative;
}























.custom_fields_holder {
	width:300px;
	
}
.custom_field {
display:block;
clear:both;
line-height:23px;
	padding-right:4px;
	text-align:left;
	
}


.custom_field span{
